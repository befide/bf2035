[data-print-preview='false'] {
  .spread__page-grid {
    z-index: 100;
    position: relative;
  }
}

[data-print-preview='true'] {
  .spread__page-grid {
    z-index: 1000;
    position: relative;
    display: grid;
    margin-top: var(--page__margin__top);
    width: var(--page__grid__width);
    height: var(--page__grid__height);
    grid-column-gap: calc(var(--grid__column-gap-in-baselines) * 1rem);
    grid-row-gap: calc(var(--grid__row-gap-in-baselines) * 1rem);
    grid-template-columns: repeat(var(--grid__column-count), calc(var(--grid__row-height-in-baselines) * 1rem));
    grid-template-rows: repeat(var(--grid__row-count), calc(var(--grid__column-width-in-baselines) * 1rem));
  }

  .header-padding {
    padding-top: 3rem;
  }

  .spread-area--university {
    grid-area: 1 / 1 / 7 / 3;
  }

  .spread-area--non-university {
    grid-area: 1 / 3 / 7 / 5;
  }

  .spread-area--nutzeranlagen {
    grid-area: 1 / 8 / 7 / 10;
  }

  .spread-area--testanlagen {
    grid-area: 1 / 10 / 7 / 13;
  }

  .spread-area--community-map {
    grid-area: 1 / 4 / 6 / 8;
  }

  .spread-area--community-1 {
    grid-area: 8 / 1 / 12 / 4;
  }

  .spread-area--community-2 {
    grid-area: 8 / 1 / 12 / 5;
  }

  .spread-area--community-3 {
    grid-area: 8 / 5 / 12 / 9;
  }

  .spread-area--community-4 {
    grid-area: 8 / 9 / 12 / 13;
  }

  .spread-area--community-5 {
    grid-area: 13 / 1 / 14 / 5;
  }

  .spread-area--community-6 {
    grid-area: 13 / 5 / 14 / 13;
  }

  .spread-area--community-table {
    grid-area: 15 / 1 / 17 / 13;
  }

  .spread-area--intro {
    grid-area: 2 / 1 / 8 / 7;
  }

  .spread-area--strategic-starting-point {
    grid-area: 1 / 9 / 9 / 13;
  }

  .spread-area--strategic-starting-point-2 {
    grid-area: 1 / 1 / 17 / 5;
    padding-top: 2rem;
  }

  .spread-area--strategic-interventions {
    grid-area: 1 / 5 / 13 / 9;
  }

  .spread-area--strategic-map {
    grid-area: 9 / 1 / 17 / 5;
    overflow: hidden;
  }

  .spread-area--aside {
    grid-area: 1 / 9 / 17 / 13;
    position: relative;
  }

  .spread--strategic-area .spread-area--aside::before {
    position: absolute;
    content: ' ';
    display: block;
    width: calc(100% + 1rem);
    height: calc(100% + 8rem);
    top: -4rem;
    left: -0.5rem;
    z-index: -1;
    background-color: rgba(black, 0.1);
  }

  .spread-area--c-1_1 {
    grid-area: 3 / 1 / 17 / 13;
  }

  .align-bottom {
    height: 100%;
    display: flex;
    align-items: flex-end;
  }

  .flex-grid {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
  }

  .spread-area--c-1_2 {
    grid-area: 1 / 1 / 17 / 7;
  }

  .spread-area--c-2_2 {
    grid-area: 1 / 7 / 17 / 13;
  }

  .spread-area--c-1_3 {
    grid-area: 1 / 1 / 17 / 5;
  }

  .under-header {
    padding-top: 10rem;
  }

  .spread-area--c-2_3 {
    grid-area: 1 / 5 / 17 / 9;

    p {
      margin-right: 1rem;
    }
  }

  .spread-area--c-3_3 {
    grid-area: 1 / 9 / 17 / 13;
  }

  .spread-area--c-1_4 {
    grid-area: 2 / 1 / 17 / 4;
  }

  .spread-area--c-2_4 {
    grid-area: 2 / 4 / 17 / 7;
    background-color: red;
  }

  .spread-area--c-3_4 {
    grid-area: 2 / 7 / 17 / 10;
    background-color: red;
  }

  .spread-area--c-4_4 {
    grid-area: 2 / 10 / 17 / 13;
    background-color: red;
  }

  .spread.spread.accelerator-type .spread-area--aside {
    grid-area: 1 / 9 / 17 / 13;
  }

  .spread--strategic-area section.footnotes {
    grid-area: 9 / 5 / 18 / 9;
    align-content: end;
  }

  .spread--strategic-area .right section.footnotes {
    display: none;
  }

  .spread-area--research-agenda-topic {
    grid-area: 11 / 1 / 17 / 7;

    img {
      width: 100%;
      height: 100%;
    }
  }

  section.footnotes {
    grid-area: 9 / 1 / 18 / 7;
    align-content: end;
    padding-bottom: 1rem;
  }

  .data-footnote-backref {
    display: none;
  }

  .right section.footnotes {
    display: none;
  }

  .spread-area--6-3-12-8 {
    grid-area: 5 / 1 / 11 / 7;
  }

  .spread-area--12-3-18-8 {
    grid-area: 5 / 7 / 11 / 13;
  }

  .spread-area--6-9-12-12 {
    grid-area: 11 / 1 / 17 / 7;
  }

  .spread-area--12-9-18-12 {
    grid-area: 11 / 7 / 17 / 13;
  }

  .spread-area--6-3-18-12 {
    grid-area: 9 / 5 / 17 / 13;
  }

  .spread-area--r-12-17 {
    grid-area: 13 / 1 / 17 / 13;
    background-color: red;
  }
}
