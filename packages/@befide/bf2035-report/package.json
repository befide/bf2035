    "remove-markdown": "0.5.0",
    "unified": "11.0.5",
    "workbox-window": "7.1.0",
    "pagedjs": "0.3.5"
  },
  "devDependencies": {
    "@astrojs/check": "0.8.1",
    "@astrojs/compiler": "2.8.2",
    "@astrojs/ts-plugin": "1.9.0",
    "@hail2u/css-mqpacker": "github:hail2u/node-css-mqpacker#v9.0.1",
    "@rollup/plugin-dsv": "3.0.4",
    "@types/d3-array": "3.2.1",
