---
import { disciplinaryProfessions, genders, careerLevels } from '@/content.config.organizations';

import fastCartesian from 'fast-cartesian';
import MemberIcon from './MemberIcon.astro';

const product = fastCartesian([
	[undefined, ...genders],
	[undefined, ...disciplinaryProfessions],
	[undefined, ...careerLevels],
]);
---

{product.map((p) => <MemberIcon careerLevel={p[0]} disciplinaryProfession={p[1]} gender={p[2]} />)}
