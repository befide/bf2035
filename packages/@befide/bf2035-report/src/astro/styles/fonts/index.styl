/* barlow-semi-condensed-latin-400-normal */
@font-face {
  font-family: 'Barlow Semi Condensed';
  font-style: normal;
  font-display: swap;
  font-weight: 400;
  src: url('/fonts/barlow-semi-condensed-latin-400-normal.woff2') format('woff2'), url('fonts/barlow-semi-condensed-latin-400-normal.woff') format('woff');
  font-feature-settings: 'smcp' 1;
  size-adjust: 80%;
}

